1.create append table
dt:string | name:string | amount:int (all can be null)
partition key: dt

2.append table create branch rt

3.append table alter table "scan.fallback-branch", "rt"

4.pk table alter table setOption("primary-key", "dt, name")

5.append table write data
Add: "20240725", "apple", 5 
Add: "20240725", "banana", 7           
NoCompact
snapshot-1

6.pk table write data
Add: "20240725", "apple", 4 
Add: "20240725", "peach", 10           
Add: "20240726", "cherry", 3 
Add: "20240726", "pear", 6           
NoCompact
snapshot-1

data-split-0-2: main branch fallback batch scan
data-split-stream: main branch stream scan
data-split-rt-0-3: rt branch batch scan


